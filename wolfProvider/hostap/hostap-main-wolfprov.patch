diff --git a/src/crypto/crypto_openssl.c b/src/crypto/crypto_openssl.c
index 2efe3ed94..202d46f48 100644
--- a/src/crypto/crypto_openssl.c
+++ b/src/crypto/crypto_openssl.c
@@ -220,7 +220,7 @@ void openssl_load_legacy_provider(void)
 	if (openssl_legacy_provider)
 		return;
 
-	openssl_legacy_provider = OSSL_PROVIDER_try_load(NULL, "legacy", 1);
+	openssl_legacy_provider = OSSL_PROVIDER_try_load(NULL, "libwolfprov", 1);
 #endif /* OpenSSL version >= 3.0 */
 }
 
@@ -247,7 +247,7 @@ static void openssl_load_default_provider_if_fips(void)
 
 	wpa_printf(MSG_DEBUG,
 		   "OpenSSL: Load default provider to replace fips provider when needed");
-	openssl_default_provider = OSSL_PROVIDER_try_load(NULL, "default", 1);
+	openssl_default_provider = OSSL_PROVIDER_try_load(NULL, "libwolfprov", 1);
 	if (!openssl_default_provider)
 		wpa_printf(MSG_DEBUG,
 			   "OpenSSL: Failed to load default provider");
